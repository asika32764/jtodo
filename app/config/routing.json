{
    "routing" : {
        "_default" : 
        {
            "pattern" : "/",
            "component" : "Todo",
            "include" : "@Todo/Config/routing.json"
        },
            
        "ComponentTodo" : 
        {
            "pattern" : "/todo",
            "component" : "@Todo"
        },
        
        "SiteTodo" : 
        {
            "pattern" : "/site/todo",
            "component" : "@SiteTodo"
        },
        
        "SiteTodo2" : 
        {
            "pattern" : "/site2/todo",
            "component" : "Site\\Todo"
        }
    }
}